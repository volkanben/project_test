<!DOCTYPE html>
<html lang="en">
<head>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
   <!-- Açıklama satırı-->
    <link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Red+Hat+Display:ital,wght@0,683;1,683&display=swap" rel="stylesheet">


    <script src="/js/quiz-test.js" type="text/javascript"></script>
    <link rel="stylesheet" href="/css/quiz_test.css">
    <link rel="stylesheet" href="/css/style.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=GFS+Didot&display=swap" rel="stylesheet">
<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=GFS+Didot&family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">
</head>
<body>
    <section id="Navbar">
        <div class="container">
            <header class="d-flex flex-wrap justify-content-center py-3 mb-4 border-bottom">
              <a href="/" class="d-flex align-items-center mb-3 mb-md-0 me-md-auto link-body-emphasis text-decoration-none">
                <svg xmlns="http://www.w3.org/2000/svg" width="40" height="40" fill="currentColor" class="bi bi-symmetry-vertical" viewBox="0 0 16 16">
                    <path d="M7 2.5a.5.5 0 0 0-.939-.24l-6 11A.5.5 0 0 0 .5 14h6a.5.5 0 0 0 .5-.5zm2.376-.484a.5.5 0 0 1 .563.245l6 11A.5.5 0 0 1 15.5 14h-6a.5.5 0 0 1-.5-.5v-11a.5.5 0 0 1 .376-.484M10 4.46V13h4.658z"/>
                  </svg>
                <span class="fs-4 title1">MİRROR</span>
              </a>
        
              <ul class="nav nav-pills">
                <li class="nav-item"><a href="#" class="nav-link active">Anasayfa</a></li>
                <li class="nav-item"><a href="#" class="nav-link">Özellikler</a></li>
                <li class="nav-item"><a href="#" class="nav-link">Hakkımızda</a></li>
                <li class="nav-item"><a href="#" class="nav-link">İletişim</a></li>
              </ul>
            </header>
          </div>
    </section>
    <div class="progress-bar">
        <div class="time">
            01:20
        </div>
        <div class="bar">
          
        </div>
    </div>
    <div class="viewBox">
        <div class="view">
            <svg viewBox="0 0 500 500" preserveAspectRatio="xMinYMin meet">
                <path d="M0,100 L100,90 200,100 L300,90 400,100 L500,90 500,100 L500,00 L0,0 Z" style="stroke: none; fill:#33a474;"></path>
            </svg>
           </div>
        <div class="title-desc">
            <span>Merhaba Volkan</span>
        </div>
        <div class="desc" id="first-desc">
            <div>
                <img src="/images/son.png" alt="Resim" width="250px" height="250px">
            </div>
            <div>
                <span>Çift cevaplı soruların cevaplarına tıklayıp 1 ve 7 arasında puanlayın</span>
            </div>
        </div>
        <div class="desc" id="second-desc" >
            <div>
                <img src="/images/EH-son.png" alt="Resim" width="250px" height="250px">
            </div>
            <div>
                <span>Evet hayır sorularını üzerlerine tıklayarak cevaplayabilirsiniz</span>
            </div>
        </div>
        </div>
        <div class="wavy-cont">

        </div>
        <div class="main-class">
            <div class="container">
                <% sorularCevaplar.forEach((soru, index) => { %>
        
                <div class="main-content">
                    <div class="board" id="<%index%>">
                        <div class="question">
                            <span id="s-question"><%= soru.soru %></span>
                        </div>
                        <div class="answers-class">
                            <form id="question-form">
                                <div class="ans-1">
                                    <div class="answer-question">
                                        <span>
                                            <a style="color:rgb(55, 54, 54); text-decoration:none; font-size: 19px;" class="aTag">
                                                <span><%= soru.secenek1 %></span>
                                            </a>
                                        </span>
                                    </div>
                                    <div class="answer-radio-1">
                                        <input type="radio" id="answer1-1<%= index %>" name="answer1<%= index %>" value="1">
                                        <label class="ans-low-1-def" for="answer1-1<%= index %>"></label>
                            
                                        <input type="radio" id="answer1-2<%= index %>" name="answer1<%= index %>" value="2">
                                        <label class="ans-low-2-def" for="answer1-2<%= index %>"></label>
                            
                                        <input type="radio" id="answer1-3<%= index %>" name="answer1<%= index %>" value="3">
                                        <label class="ans-low-3-def" for="answer1-3<%= index %>"></label>
                            
                                        <input type="radio" id="answer1-4<%= index %>" name="answer1<%= index %>" value="4">
                                        <label class="ans-mid-def" for="answer1-4<%= index %>"></label>
                            
                                        <input type="radio" id="answer1-5<%= index %>" name="answer1<%= index %>" value="5">
                                        <label class="ans-high-1-def" for="answer1-5<%= index %>"></label>
                            
                                        <input type="radio" id="answer1-6<%= index %>" name="answer1<%= index %>" value="6">
                                        <label class="ans-high-2-def" for="answer1-6<%= index %>"></label>
                            
                                        <input type="radio" id="answer1-7<%= index %>" name="answer1<%= index %>" value="7">
                                        <label class="ans-high-3-def" for="answer1-7<%= index %>"></label>
                                    </div>
                                </div>
                                <div class="ans-2">
                                    <div class="answer-question">
                                        <span>
                                            <a style="color:rgb(55, 54, 54); text-decoration:none; font-size: 19px;" class="bTag">
                                                <span><%= soru.secenek2 %></span>
                                            </a>
                                        </span>
                                    </div>
                                    <div class="answer-radio-2">
                                        <input type="radio" id="answer2-1<%= index %>" name="answer2<%= index %>" value="1">
                                        <label class="ans-low-1-def" for="answer2-1<%= index %>"></label>
                            
                                        <input type="radio" id="answer2-2<%= index %>" name="answer2<%= index %>" value="2">
                                        <label class="ans-low-2-def" for="answer2-2<%= index %>"></label>
                            
                                        <input type="radio" id="answer2-3<%= index %>" name="answer2<%= index %>" value="3">
                                        <label class="ans-low-3-def" for="answer2-3<%= index %>"></label>
                            
                                        <input type="radio" id="answer2-4<%= index %>" name="answer2<%= index %>" value="4">
                                        <label class="ans-mid-def" for="answer2-4<%= index %>"></label>
                            
                                        <input type="radio" id="answer2-5<%= index %>" name="answer2<%= index %>" value="5">
                                        <label class="ans-high-1-def" for="answer2-5<%= index %>"></label>
                            
                                        <input type="radio" id="answer2-6<%= index %>" name="answer2<%= index %>" value="6">
                                        <label class="ans-high-2-def" for="answer2-6<%= index %>"></label>
                            
                                        <input type="radio" id="answer2-7<%= index %>" name="answer2<%= index %>" value="7">
                                        <label class="ans-high-3-def" for="answer2-7<%= index %>"></label>
                                    </div>
                                </div>
                            </form>
                            
                        </div>
                        
                    </div>
                </div>
                <hr>
                <% }); %>
                <% evet_hayir.forEach(ques => { %>
                <div class="main-content">
                    <div class="board">
                        <div class="question">
                            <span id="s-question"><%=ques.bsoru%></span>
                        </div>
                        <div class="answers-class-y">
                            <div id="ans-1">
                                <span><a style="text-decoration:none;font-size: 25px; color: green;" id="aTag">Evet</a></span>
                            </div>
                            <div id="ans-2">
                                <span><a id="bTag" style="text-decoration:none; font-size:25px; color:purple">Hayır</a></span>
                            </div>
                        </div>
                    </div>
                </div>
                <hr>
                <% }) %>
            </div>
        </div>
        
    </div>
</body>
</html>
